{
  "expo": {
    "name": "smkalazharabsen",
    "slug": "smkalazharabsen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/absen.jpg",
    "scheme": "smkalazharabsen",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/absen.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/absen.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/absen.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7582a3aa-fb9d-48c8-9c1f-d2297c6dbf24"
      }
    },
    "owner": "absen_school"
  }
}
